    "tertiary": {
      "value": "",
      "description": "Whether it's a three-level button",
      "default": "false",
      "type": "boolean",
      "description_zh": "是否是三级按钮"
    },
    "text": {
      "value": "",
      "description": "Whether or not to display as a text button",
      "default": "false",
      "type": "boolean",
      "description_zh": "是否显示为文本按钮"
    },
    "text-color": {
      "value": "",
      "description": "Button text color (supports colors like '#FFF', '#FFFFFF', 'yellow', 'rgb(0, 0, 0)')",
      "default": "undefined",
      "type": "string",
      "description_zh": "按钮文字颜色（支持形如 `#FFF`， `#FFFFFF`， `yellow`，`rgb(0, 0, 0)` 的颜色）"
    },
    "type": {
      "value": [
        "default",
        "tertiary",
        "primary",
        "success",
        "info",
        "warning",
        "error"
      ],
      "description": "The type of button",
      "default": "default",
      "type": "default / tertiary / primary / success / info / warning / error",
      "description_zh": "按钮的类型"
    },
    "tag": {
      "value": "",
      "description": "The button needs to be rendered to be labeled as why",
      "default": "button",
      "type": "string",
      "description_zh": "按钮需要被渲染为什么标签"
    }
